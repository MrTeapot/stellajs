{
  "name": "@mrteapot/stellajs",
  "version": "0.0.17",
  "description": "A nodejs framework for building REST apis",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "test:unit": "NODE_PATH=./src mocha ./test/unit/**/*.spec.ts --exit || exit 0",
    "test:integration": "NODE_PATH=./src mocha ./test/integration/**/*.spec.ts --exit || exit 0",
    "test:unit:coverage": "nyc npm run test:unit --exit || exit 0",
    "test:integration:coverage": "nyc npm run test:integration --exit || exit 0"
  },
  "files": [
    "dist",
    "./dist/index.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "mrteapot",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrTeapot/stellajs.git"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/json-pointer": "^1.0.30",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.6.0",
    "@types/proxyquire": "^1.3.28",
    "@types/reflect-metadata": "^0.1.0",
    "@types/sinon-chai": "^3.2.4",
    "@types/supertest": "^2.0.10",
    "chai": "^4.2.0",
    "install": "^0.13.0",
    "mocha": "^8.1.1",
    "npm": "^6.14.8",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^9.0.3",
    "sinon-chai": "^3.5.0",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "ajv": "^6.12.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "fastify": "^3.2.1",
    "helmet": "^4.1.0",
    "json-pointer": "^0.6.0",
    "middie": "^5.1.0",
    "path-to-regexp": "^6.1.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.3.0"
  }
}
